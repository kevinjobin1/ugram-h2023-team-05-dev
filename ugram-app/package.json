{
  "name": "ugram-frontend",
  "version": "0.0.1",
  "description": "Vite+React Frontend for Ugram project",
  "private": true,
  "license": "MIT",
  "author": "GLO3112-team-05",
  "keywords": [
    "vite",
    "react",
    "ugram",
    "glo3112"
  ],
  "bugs": {
    "url": "https://github.com/GLO3112-classrooms/ugram-h2023-team-05/issues"
  },
  "homepage": "https://ugram.link",
  "main": "dist/index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/GLO3112-classrooms/ugram-h2023-team-05"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start:dev": "vite --host",
    "start": "vite --open",
    "start:prod": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "format+lint": "npm run format && npm run lint",
    "type-check": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cover": "vitest run --coverage",
    "deploy": "npm install && vite build && vite preview --port 5173"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.272.0",
    "@chakra-ui/react": "^2.5.1",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.9.11",
    "@mui/material": "^5.11.13",
    "@mui/x-date-pickers": "^5.0.19",
    "@sentry/react": "^7.48.0",
    "@sentry/tracing": "^7.43.0",
    "axios": "^1.3.3",
    "dotenv": "^16.0.3",
    "emoji-mart": "^5.5.2",
    "framer-motion": "^9.0.2",
    "mui-image": "^1.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^4.7.4",
    "react-ga": "^3.3.1",
    "react-hook-form": "^7.43.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.8.1",
    "react-webcam": "^7.0.1",
    "socket.io-client": "^4.6.1",
    "swiper": "^9.1.0",
    "vite-plugin-svgr": "^2.4.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@mui/icons-material": "^5.11.9",
    "@mui/styled-engine-sc": "^5.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/mui-image": "^1.0.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "happy-dom": "^8.7.2",
    "https": "^1.0.0",
    "jsdom": "^21.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.5.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vite-plugin-fonts": "^0.7.0",
    "vitest": "^0.28.5"
  },
  "pre-commit": [
    "test",
    "lint",
    "format"
  ]
}
